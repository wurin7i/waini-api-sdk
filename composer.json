{
    "name": "waini/api-sdk",
    "description": "Whatsapp API (unofficial) SDK for PHP",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "whatsapp",
        "wa",
        "waini",
        "chat",
        "message",
        "messaging",
        "gateway",
        "api",
        "sdk"
    ],
    "authors": [
        {
            "name": "Wuri Nugrahadi",
            "email": "wuri.nugrahadi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "firebase/php-jwt": "^6.11 || ^7.0",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "ramsey/devtools": "^2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WuriN7i\\ApiSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WuriN7i\\Test\\ApiSdk\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "ramsey/composer-repl": true,
            "ramsey/devtools": true
        },
        "sort-packages": true
    },
    "extra": {
        "captainhook": {
            "force-install": true
        },
        "ramsey/conventional-commits": {
            "configFile": "conventional-commits.json"
        },
        "ramsey/devtools": {
            "command-prefix": "dev",
            "memory-limit": "-1"
        }
    }
}
